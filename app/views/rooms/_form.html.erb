<%= form_for @room do |f| %>
            
    <%= f.label :Home %>
    <%= f.select :home_id, current_user.homes.collect{ |c| [c.home_name, c.id] } %>
            
    <%= f.label :room_name %>
    <%= f.text_field :room_name %>
    <div class="modal-footer">
    <%= f.submit class:"btn btn-success" %>
    </div> 
<% end %>