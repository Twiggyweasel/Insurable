<h1>Hi <%= @user.name %></h1>
  <% @user.homes.each do |home| %>
    <div class="col-lg-12 center">
    <h2><%= link_to home.home_name, "", class: "btn btn-lg btn-primary center" %></h2>
    </div>
    
    <% @user.rooms.each do |room| %>
    
      <div class="row">
        <div class="col-lg-12 center">
        <h4><%= link_to room.room_name, "" %></h4>
        </div>
        <div class="row">
          <% room.items.each do |item| %>
          <div class="col-lg-12 center">
          <h6><%= item.item_name %></h6>
          </div>
          <% end %>
        </div>
          
      </div>
    
    <% end %>
    
  <% end %>
  
  <table class="table table-hover">
    <thead>
    <tr>
      <th>Room</th>
      <th># Of Items</th>
      <th>Total Value</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <td>Living Room</td>
      <td>1</td>
      <td>$75</td>
    </tr>
    <tr>  
      <td>Kitchen</td>
      <td>0</td>
      <td>$0</td>    
    </tr>
    </tbody>
  </table>