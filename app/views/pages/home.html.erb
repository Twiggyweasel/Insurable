<div class='row'>
  <br>
  <% if !logged_in? %>
    <div class='jumbotron'>
      <h1 class="center">Welcome to Insurable!</h1>
      <p class="center">Making cataloging your personal items simple and fun!</p>
      <div class="center">
        <%= link_to "Signup Now!", register_path, data: { modal: true }, class: "btn btn-primary btn-lg" %>
      </div>
    </div>
  <% else %>
  <div class="col-lg-12 center">
    <h1>Hello</h1>
    <hr/>
  </div>
  
  <div class='row'>
    <div class='col-md-3'>
      <div class='well'>
        <h2>This is where Homes and Rooms will be displayed</h2>
      </div>
    </div>
    <div class='col-md-9'>
      <div class='well'>
        <h2>This is where Items will be displayed</h2>
      </div>
    </div>
  </div>
  
  <div class="wrapper">
  <div class="content-main">...</div>
  <div class="content-secondary">...</div>
</div>
  <% end %>
</div>

<div class="modal fade" id="registerModal" tabindex="-1" role="dialog" aria-labelledby="registerModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
  </div>
</div>

