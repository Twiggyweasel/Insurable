<h1 class="col-md-4 col-md-offset-4 center">Login</h1>

<div class="row">
    <div class="well col-md-6 col-md-offset-3">
      <%= form_tag '/login' do %>
        <%= label_tag :email %>
        <%= text_field_tag :email %>
        
        <%= label_tag :password %>
        <%= password_field_tag :password %>
        <%= link_to "Fogotten Password", new_password_reset_path %>
        <%= submit_tag 'Login', class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>